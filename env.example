APP_NAME="BacklinkForge"
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost:8000

LOG_CHANNEL=stack
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=backlinkforge
DB_USERNAME=root
DB_PASSWORD=password

BROADCAST_DRIVER=log
CACHE_DRIVER=redis
FILESYSTEM_DISK=local
QUEUE_CONNECTION=redis
SESSION_DRIVER=redis
SESSION_LIFETIME=120

MEMCACHED_HOST=127.0.0.1

REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_MAILER=smtp
MAIL_HOST=mailhog
MAIL_PORT=1025
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_ENCRYPTION=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

PUSHER_APP_ID=
PUSHER_APP_KEY=
PUSHER_APP_SECRET=
PUSHER_HOST=
PUSHER_PORT=443
PUSHER_SCHEME=https
PUSHER_APP_CLUSTER=mt1

VITE_APP_NAME="${APP_NAME}"
VITE_PUSHER_APP_KEY="${PUSHER_APP_KEY}"
VITE_PUSHER_HOST="${PUSHER_HOST}"
VITE_PUSHER_PORT="${PUSHER_PORT}"
VITE_PUSHER_SCHEME="${PUSHER_SCHEME}"
VITE_PUSHER_APP_CLUSTER="${PUSHER_APP_CLUSTER}"

# BacklinkForge Specific Configuration

# Automation Service
PLAYWRIGHT_SERVICE_URL=http://localhost:3000
AUTOMATION_SERVICE_SECRET=your-automation-service-secret

# Proxy Configuration
PROXY_PROVIDER_API_KEY=
PROXY_ROTATION_STRATEGY=round_robin
PROXY_HEALTH_CHECK_INTERVAL=300

# Captcha Services
CAPTCHA_2CAPTCHA_KEY=
CAPTCHA_ANTICAPTCHA_KEY=
CAPTCHA_TESSERACT_ENABLED=true
CAPTCHA_FALLBACK_SERVICE=2captcha

# Content Generation
OPENAI_API_KEY=
AI_CONTENT_ENABLED=true
AI_MAX_TOKENS_PER_REQUEST=1000
SPINTAX_ENABLED=true

# Storage (S3/MinIO)
FILESYSTEM_DISK=s3
AWS_ENDPOINT=http://localhost:9000
AWS_BUCKET=backlinkforge
AWS_ACCESS_KEY_ID=minioadmin
AWS_SECRET_ACCESS_KEY=minioadmin
AWS_DEFAULT_REGION=us-east-1
AWS_USE_PATH_STYLE_ENDPOINT=true

# Queue Configuration
QUEUE_CONNECTION=redis
QUEUE_FAILED_DRIVER=database-uuids
HORIZON_PREFIX=backlinkforge

# Security
SESSION_SECURE_COOKIE=false
SANCTUM_STATEFUL_DOMAINS=localhost:8000
SESSION_DOMAIN=localhost

# Monitoring
SENTRY_LARAVEL_DSN=
SENTRY_TRACES_SAMPLE_RATE=1.0

# Billing (Stripe)
STRIPE_KEY=
STRIPE_SECRET=
STRIPE_WEBHOOK_SECRET=
CASHIER_CURRENCY=usd
CASHIER_CURRENCY_LOCALE=en

# Email Configuration
MAIL_MAILER=smtp
MAIL_HOST=mailhog
MAIL_PORT=1025
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_ENCRYPTION=null
MAIL_FROM_ADDRESS="noreply@backlinkforge.com"
MAIL_FROM_NAME="BacklinkForge"

# Development Settings
APP_DEBUG=true
LOG_LEVEL=debug
CACHE_DRIVER=redis
SESSION_DRIVER=redis
QUEUE_CONNECTION=redis 